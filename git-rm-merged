#!/bin/bash

eval "$(git for-each-ref refs/heads/ --merged master --shell --format='
set -eu
ref=%(refname)
sha=%(objectname)
shortref=%(refname:short)
shortsha=%(objectname:short)
if [ "$ref" != "refs/heads/master" ]; then
  git update-ref -d "$ref" "$sha"
  echo "Deleted $shortref ($shortsha)"
fi
')"
